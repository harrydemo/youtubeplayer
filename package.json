{
    "name": "youtubeplayer",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "build:prod": "ng build --prod",
        "build:env": "node config/build-env.js",
        "postinstall": "npm rebuild node-sass",
        "copy:heroku": "shx cp -r ./config/heroku/*.* ./dist",
        "copy:domain": "shx cp CNAME ./dist",
        "copy:package": "shx cp ./package.json ./dist/assets/",
        "test": "ng test",
        "test:ci": "ng test --single-run",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "clean": "rimraf -- node_modules",
        "server:prod": "http-server ./dist -a localhost -p 4200"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/core": "0.0.29",
        "@angular/animations": "^5.2.0",
        "@angular/common": "^5.2.0",
        "@angular/compiler": "^5.2.0",
        "@angular/core": "^5.2.0",
        "@angular/forms": "^5.2.0",
        "@angular/http": "^5.2.0",
        "@angular/platform-browser": "^5.2.0",
        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "core-js": "^2.4.1",
        "http-server": "0.10.0",
        "replace": "0.3.0",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "~1.7.2",
        "@angular/compiler-cli": "^5.2.0",
        "@angular/language-service": "^5.2.0",
        "@ngrx/effects": "4.1.1",
        "@ngrx/router-store": "4.1.1",
        "@ngrx/store": "4.1.1",
        "@ngrx/store-devtools": "4.1.1",
        "@types/gapi": "0.0.35",
        "@types/gapi.auth2": "0.0.46",
        "@types/gapi.youtube": "3.0.34",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "~6.0.60",
        "@types/youtube": "0.0.31",
        "bootstrap-sass": "3.3.7",
        "codelyzer": "^4.0.1",
        "font-awesome": "4.7.0",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-spec-reporter": "0.0.32",
        "ngrx-action-creator-factory": "0.0.1",
        "ngrx-store-localstorage": "0.2.4",
        "ngx-infinite-scroll": "^0.8.2",
        "ngx-tooltip": "0.0.9",
        "ngx-typeahead": "0.1.0",
        "ngx-youtube-player": "^0.1.0",
        "protractor": "~5.1.2",
        "rimraf": "2.6.1",
        "shx": "0.2.2",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "~2.5.3"
    }
}